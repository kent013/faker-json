{
    "name": "kent013/faker-json",
    "description": "Call faker with json",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "FakerJson\\": "src/FakerJson/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "kent013",
            "email": "kentaro.ishitoya@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "fakerphp/faker": "^1.20",
        "haydenpierce/class-finder": "^0.4.4",
        "webmozart/assert": "^1.11",
        "phpstan/phpdoc-parser": "^1.16"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "friendsofphp/php-cs-fixer": "^3.13",
        "phpstan/phpstan-webmozart-assert": "^1.2",
        "phpstan/extension-installer": "^1.2"
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}
